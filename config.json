[
{ "simplemodule": {
	"name": "simple",
	"gates": [ { "gate": "k" } ]
}},

{ "module": {
	"name": "net",
	"params": [ { "k": "2" } ],
	"submodules": [
	{
		"name": "simplevec",
		"module": "simple",
		"size": "4",
		"params": [ { "x": "6" } ]
	},
	{
		"if": "qqq > 3",
		"subm":
		{
			"name": "condvec",
			"module": "simple"
		}
	}],
	"connections": [
	{
		"loop": "i",
		"start": "0",
		"end": "2",
		"conn": {
			"if": "i < 2",
			"conn": {
				"loop": "j",
				"start": "0",
				"end": "1",
				"conn": {
					"from": "simplevec[2*i].gate[j]",
					"to": "simplevec[2*i+1].gate[j]"
				}
			}
		}
	}],
	"gates": [ { "out": "2" } ]
}},

{ "network": {
	"module": "net",
	"params": [ { "w": "4" }, { "qqq": "10" } ]
}}
]
